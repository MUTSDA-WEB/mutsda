{
   "name": "server",
   "module": "index.js",
   "devDependencies": {
      "@types/bun": "latest"
   },
   "peerDependencies": {
      "typescript": "^5"
   },
   "scripts": {
      "spin": "bun --hot server.js",
      "chat": "bun --bun run worker.js && bun --bun run realtime.js",
      "build": "bun pdeploy && bun spin",
      "pdev": "bunx prisma dev",
      "pmigrate": "bunx --bun prisma migrate dev",
      "pgen": "bunx prisma generate",
      "pweb": "bunx --bun prisma studio",
      "pdeploy": "bunx --bun prisma migrate deploy"
   },
   "type": "module",
   "dependencies": {
      "@prisma/adapter-pg": "^7.2.0",
      "@prisma/client": "^7.3.0",
      "hono": "^4.11.3",
      "ioredis": "^5.9.2",
      "pg": "^8.16.3",
      "prisma": "^7.3.0",
      "socket.io": "^4.8.3"
   }
}
