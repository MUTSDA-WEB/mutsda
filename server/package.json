{
   "name": "server",
   "module": "index.js",
   "devDependencies": {
      "@types/bun": "latest"
   },
   "peerDependencies": {
      "typescript": "^5"
   },
   "scripts": {
      "spin": "bun --hot server.js",
      "worker": "bun --bun run worker.js",
      "realtime": "bun --bun run realtime.js",
      "build": "bun run pdeploy && bun spin",
      "pdev": "bun x prisma dev",
      "pmigrate": "bun x prisma migrate dev",
      "pgen": "bun x prisma generate",
      "pweb": "bun x prisma studio",
      "pdeploy": "bun x  --bun prisma migrate deploy"
   },
   "type": "module",
   "dependencies": {
      "@prisma/adapter-pg": "^7.2.0",
      "@prisma/client": "^7.3.0",
      "cloudinary": "^2.9.0",
      "hono": "^4.11.3",
      "ioredis": "^5.9.2",
      "nodemailer": "^8.0.1",
      "pg": "^8.16.3",
      "prisma": "^7.3.0",
      "socket.io": "^4.8.3"
   }
}
